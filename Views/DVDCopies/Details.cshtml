

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Copies Model</h4>
    <hr />
    <dl class="row">
        @foreach (var item in Model)
        {
            <dt class = "col-sm-2">
                Member Name
            </dt>
            <dd class = "col-sm-10">
               @item.MemberName
            </dd>
            <dt class = "col-sm-2">
               Loan
            </dt>
            <dd class = "col-sm-10">
               @item.Loan.DateOut
            </dd>
                                                                                                                                                                                                                                                                                          
            
        }
    </dl>
</div>


<div>
    <a asp-action="Index">Back to List</a>
</div>
