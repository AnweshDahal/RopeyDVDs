

@{
    ViewData["Title"] = "Checkout";
}

<h1>Details</h1>

<div>
    <h4>Loan Checkout</h4>
    <hr />
    <dl class="row">
        @foreach (var item in Model)
        {
            <dt class = "col-sm-2">
                Loan Number
            </dt>
            <dd class = "col-sm-10">
               @item.LoanNumber
            </dd>
            <dt class = "col-sm-2">
                Date Out
            </dt>
            <dd class = "col-sm-10">
               @item.DateOut
            </dd>                                                                                                                                                                                                                                                                                           
        }
    </dl>
</div>


<div>
    <a asp-action="Index">Back to List</a>
</div>
