@model RopeyDVDs.Models.MembershipCategory

@{
    ViewData["Title"] = "Edit Membership Category";
}

<h1>Edit Membership Category </h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="MembersgipCategoryDescription" class="control-label">Enter Membership Category Description</label>
                <input asp-for="MembersgipCategoryDescription" class="form-control" />
                <span asp-validation-for="MembersgipCategoryDescription" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MembershipCategoryTotalLoans" class="control-label">Enter Membership Category Total Loans</label>
                <input asp-for="MembershipCategoryTotalLoans" class="form-control" />
                <span asp-validation-for="MembershipCategoryTotalLoans" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
