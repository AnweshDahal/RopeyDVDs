@model IEnumerable<RopeyDVDs.Models.MembershipCategory>

@{
    ViewData["Title"] = "Membership Category List";
}

<h1>Membership Category List</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
               Membership Category Description
            </th>
            <th>
               Membership Category Total Loans
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MembersgipCategoryDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MembershipCategoryTotalLoans)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
